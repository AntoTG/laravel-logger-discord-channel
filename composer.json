{
  "name": "antotg/laravel-logger-discord-channel",
  "description": "A Discord based Monolog driver for Laravel",
  "keywords": [
    "laravel",
    "logger",
    "discord"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Anto TG",
      "email": "contact@antotg.com"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "illuminate/contracts": "~11.0",
    "illuminate/queue": "~11.0",
    "illuminate/support": "~11.0",
    "guzzlehttp/guzzle": "^7.7.0",
    "monolog/monolog": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "antotg\\LoggerDiscordChannel\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
